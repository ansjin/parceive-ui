{
  "jsDeps": [
      "./app/bower_components/jquery/dist/jquery.js",
      "./app/bower_components/jquery-hoverIntent/jquery.hoverIntent.js",
      "./app/bower_components/angular/angular.js",
      "./app/bower_components/angular/angular-route.js",
      "./app/bower_components/d3/d3.js",
      "./app/bower_components/lodash/lodash.js",
      "./app/bower_components/rsvp/rsvp.js",
      "./app/bower_components/jQuery-contextMenu/dist/jquery.ui.position.js",
      "./app/bower_components/jQuery-contextMenu/dist/jquery.contextMenu.js",
      "./app/bower_components/code-prettify/src/prettify.js"
    ],

  "cssDeps": [
    "./app/bower_components/jQuery-contextMenu/dist/jquery.contextMenu.css",
    "./app/bower_components/code-prettify/src/prettify.css"
  ],

  "codeOrder": [
    "app/scripts/module_interceptor.js",
    "app/scripts/app.js",
    "app/scripts/**/*.js"
  ],

  "testOrder": [
    "app/tests/config.js",
    "app/tests/**/*.js",
    "app/tests/run.js"
  ],

  "opts": {
    "minify": false,
    "sourcemaps": true,
    "failonlint": false
  }
}
